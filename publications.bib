	date = {2021-04-01},
	langid = {english},
	keywords = {Machine learning, Deep learning, Convolutional neural networks, Arrhythmia detection and identification, Artificial Neural Networks, Condition monitoring, Fault detection, Structural damage detection, Structural health monitoring},
