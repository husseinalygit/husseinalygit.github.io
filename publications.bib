	author = {Nandkeolyar, Shobhit and Ray, Pravat Kumar},
	urldate = {2023-11-05},
	date = {2022-07-01},
	keywords = {Machine learning, Smart grid, Demand side management, Electric vehicle, Demand response, {EV} charging, Hidden Markov Model},
