	keywords = {Ensemble deep random vector functional link ({edRVFL}), Pruning, {UCI} classification datasets, Weighting methods},
	file = {1-s2.0-S0031320322003600-main.pdf:D\:\\Zetero\\storage\\YMVBDP5M\\1-s2.0-S0031320322003600-main.pdf:application/pdf;ScienceDirect Snapshot:D\:\\Zetero\\storage\\YES6MI6P\\S0031320322003600.html:text/html;Shi et al_2022_Weighting and pruning based ensemble deep random vector functional link network.pdf:D\:\\Zetero\\storage\\5WJMZU8Y\\1-s2.0-S0031320322003600-main.pdf:application/pdf},
