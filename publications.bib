	langid = {english},
	keywords = {Accelerometers, Annotated food pictures, Diabetes management, {ECG}, Glucose, Wearable devices},
	file = {ScienceDirect Full Text PDF:D\:\\Zetero\\storage\\WACZAG3F\\Dubosson et al. - 2018 - The open D1NAMO dataset A multi-modal dataset for.pdf:application/pdf;ScienceDirect Snapshot:D\:\\Zetero\\storage\\XDPN9DFM\\S2352914818301059.html:text/html},
