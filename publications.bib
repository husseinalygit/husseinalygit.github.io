	eprint = {2201.05809 [cs]},
	note = {type: article},
	keywords = {Computer Science - Machine Learning, Computer Science - Computer Vision and Pattern Recognition, Computer Science - Artificial Intelligence, Computer Science - Neural and Evolutionary Computing},
	file = {arXiv.org Snapshot:D\:\\Zetero\\storage\\JRMZ8SGY\\2201.html:text/html;Shi et al_2022_Weighting and Pruning based Ensemble Deep Random Vector Functional Link Network.pdf:D\:\\Zetero\\storage\\567PN69V\\Shi et al_2022_Weighting and Pruning based Ensemble Deep Random Vector Functional Link Network.pdf:application/pdf},
