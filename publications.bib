	author = {Zarpelão, Bruno Bogaz and Miani, Rodrigo Sanches and Kawakani, Cláudio Toshio and de Alvarenga, Sean Carlisto},
	urldate = {2021-04-03},
	date = {2017-04-15},
	langid = {english},
	keywords = {Internet of Things, Cybersecurity, Intrusion detection system},
}

@article{yu_network_2017,
	title = {Network Intrusion Detection through Stacking Dilated Convolutional Autoencoders},
