	date = {2021-05},
	note = {{ISSN}: 2375-1207},
	keywords = {Training, Deep learning, Games, Privacy, deep-learning, Differential privacy, Differentially-private, Differentially-private-({DP})-machine-learning, {DP}-{SGD}, machine-learning, membership-inference, Toxicology, Upper bound},
	file = {IEEE Xplore Abstract Record:D\:\\Zetero\\storage\\EBCHGHIQ\\9519424.html:text/html;Nasr et al_2021_Adversary Instantiation.pdf:D\:\\Zetero\\storage\\UJ3DVK3R\\Nasr et al_2021_Adversary Instantiation.pdf:application/pdf},
