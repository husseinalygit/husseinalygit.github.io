	file = {IEEE Xplore Abstract Record:D\:\\Zetero\\storage\\ADSXDKWW\\8384130.html:text/html;IEEE Xplore Full Text PDF:D\:\\Zetero\\storage\\EBFXZZ2B\\Afrin and Kwasinski - 2017 - A privacy-preserving method with flexible charging.pdf:application/pdf},
}

