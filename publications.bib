	langid = {english},
	keywords = {\_tablet\_modified, Broad Learning System, Fuzzy Broad Learning System, Kernel Random Vector Functional Link Network, Multilabel classification, Multilabel Randomized Neural Networks, Noniterative learning, Random Vector Functional Link Network},
