	eprint = {1512.03385 [cs]},
	keywords = {Computer Science - Computer Vision and Pattern Recognition},
	file = {arXiv Fulltext PDF:D\:\\Zetero\\storage\\PBGCUIZG\\He et al. - 2015 - Deep Residual Learning for Image Recognition.pdf:application/pdf;arXiv.org Snapshot:D\:\\Zetero\\storage\\M2N8QDL8\\1512.html:text/html},
