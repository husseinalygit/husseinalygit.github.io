Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {non-invasive, continuous monitoring, {FDA}, fluorescence, glucose, {ISO} 15197, {MARD}, metabolic heat conformation, minimally-invasive, plasmon resonance, spectroscopy, ultrasound},
