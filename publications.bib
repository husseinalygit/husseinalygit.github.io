	keywords = {Electric vehicle, Demand response, Charging pattern, Flexibility, Non-intrusive load extracting algorithm},
	file = {ScienceDirect Full Text PDF:D\:\\Zetero\\storage\\FPWYB9KB\\Zhao et al. - 2019 - Quantifying flexibility of residential electric ve.pdf:application/pdf;ScienceDirect Snapshot:D\:\\Zetero\\storage\\92E2ZYS2\\S2210670719306304.html:text/html},
}

