	author = {Sohankar, J. and Sadeghi, K. and Banerjee, A. and Gupta, S. K. S.},
	date = {2017-08},
	keywords = {Feature extraction, security of data, biometrics, biometrics (access control), Authentication, bio-electrical signals, biometric security systems, biometric systems-liveness detection methods, Face, face image, fake input data detection, fingerprint identification, fingerprint image, liveness detection, Physiology, presentation attack, presentation attacks, Task analysis, Usability},
	file = {IEEE Xplore Abstract Record:D\:\\Zetero\\storage\\DRNT6Z6Z\\8397608.html:text/html;Sohankar et al. - 2017 - Systematic analysis of liveness detection methods .pdf:D\:\\Zetero\\storage\\R3F4VU32\\Sohankar et al. - 2017 - Systematic analysis of liveness detection methods .pdf:application/pdf},
}

