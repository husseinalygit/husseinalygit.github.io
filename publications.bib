	note = {Number: 1
Publisher: Nature Publishing Group},
	file = {Full Text PDF:D\:\\Zetero\\storage\\KRTRYKFE\\Haythorne et al. - 2019 - Diabetes causes marked inhibition of mitochondrial.pdf:application/pdf;Snapshot:D\:\\Zetero\\storage\\IWFLSD9C\\s41467-019-10189-x.html:text/html},
