	langid = {english},
	note = {Publisher: Hindawi},
	file = {Full Text PDF:D\:\\Zetero\\storage\\XJG8U2XQ\\Alotaiby et al. - 2020 - A Nonfiducial PPG-Based Subject Authentication App.pdf:application/pdf;Snapshot:D\:\\Zetero\\storage\\5PKYBTDM\\8849845.html:text/html},
}

@inproceedings{zhao_your_2018-1,
