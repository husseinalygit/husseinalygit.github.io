	date = {2023-05},
	note = {Conference Name: {IEEE} Transactions on Industrial Informatics},
	keywords = {Informatics, Deep learning, Optimization, Benchmark testing, Best practices, deep neural networks ({DNNs}), load disaggregation, machine learning ({ML}), {NILMTK}, nonintrusive load monitoring ({NILM}), Pipelines, Scholarships},
