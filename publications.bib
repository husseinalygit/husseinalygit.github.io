	keywords = {Deep learning, Kernel density estimator, Probabilistic forecasting, Residential electric vehicle},
	file = {ScienceDirect Full Text PDF:D\:\\Zetero\\storage\\BDFHPA6F\\Jamali Jahromi et al. - 2022 - Probability density function forecasting of reside.pdf:application/pdf;ScienceDirect Snapshot:D\:\\Zetero\\storage\\JNYK4T4T\\S0306261922009205.html:text/html},
