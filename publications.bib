}

@article{acharya_cybersecurity_2020,
	title = {Cybersecurity of Smart Electric Vehicle Charging: A Power Grid Perspective},
	volume = {8},
	issn = {2169-3536},
	url = {https://ieeexplore.ieee.org/abstract/document/9272723},
