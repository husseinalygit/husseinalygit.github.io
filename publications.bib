@article{zhang_visual_2017,
	title = {Visual Tracking With Convolutional Random Vector Functional Link Network},
	volume = {47},
	issn = {2168-2275},
	doi = {10.1109/TCYB.2016.2588526},
	abstract = {Deep neural network-based methods have recently achieved excellent performance in visual tracking task. As very few training samples are available in visual tracking task, those approaches rely heavily on extremely large auxiliary dataset such as {ImageNet} to pretrain the model. In order to address the discrepancy between the source domain (the auxiliary data) and the target domain (the object being tracked), they need to be finetuned during the tracking process. However, those methods suffer from sensitivity to the hyper-parameters such as learning rate, maximum number of epochs, size of mini-batch, and so on. Thus, it is worthy to investigate whether pretraining and fine tuning through conventional back-prop is essential for visual tracking. In this paper, we shed light on this line of research by proposing convolutional random vector functional link ({CRVFL}) neural network, which can be regarded as a marriage of the convolutional neural network and random vector functional link network, to simplify the visual tracking system. The parameters in the convolutional layer are randomly initialized and kept fixed. Only the parameters in the fully connected layer need to be learned. We further propose an elegant approach to update the tracker. In the widely used visual tracking benchmark, without any auxiliary data, a single {CRVFL} model achieves 79.0\% with a threshold of 20 pixels for the precision plot. Moreover, an ensemble of {CRVFL} yields comparatively the best result of 86.3\%.},
	pages = {3243--3253},
	number = {10},
	journaltitle = {{IEEE} Transactions on Cybernetics},
	author = {Zhang, Le and Suganthan, Ponnuthurai Nagaratnam},
	date = {2017-10},
	note = {Conference Name: {IEEE} Transactions on Cybernetics},
	keywords = {deep learning, Data models, Biological neural networks, Training, Convolutional neural network ({CNN}), convolutional random vector functional link ({CRVFL}), random vector functional link ({RVFL}), Target tracking, visual tracking, Visualization},
	file = {IEEE Xplore Abstract Record:D\:\\Zetero\\storage\\JPQ8VGNC\\7543468.html:text/html;Zhang_Suganthan_2017_Visual Tracking With Convolutional Random Vector Functional Link Network.pdf:D\:\\Zetero\\storage\\S4W9WGR8\\Zhang_Suganthan_2017_Visual Tracking With Convolutional Random Vector Functional Link Network.pdf:application/pdf},
}

@article{ateniese_hacking_2015,
	title = {Hacking smart machines with smarter ones: How to extract meaningful data from machine learning classifiers},
	volume = {10},
	issn = {1747-8405},
	url = {https://www.inderscienceonline.com/doi/abs/10.1504/IJSN.2015.071829},
	doi = {10.1504/IJSN.2015.071829},
	shorttitle = {Hacking smart machines with smarter ones},
	abstract = {Machine-learning ({ML}) enables computers to learn how to recognise patterns, make unintended decisions, or react to a dynamic environment. The effectiveness of trained machines varies because of more suitable {ML} algorithms or because superior training sets. Although {ML} algorithms are known and publicly released, training sets may not be reasonably ascertainable and, indeed, may be guarded as trade secrets. In this paper we focus our attention on {ML} classifiers and on the statistical information that can be unconsciously or maliciously revealed from them. We show that it is possible to infer unexpected but useful information from {ML} classifiers. In particular, we build a novel meta-classifier and train it to hack other classifiers, obtaining meaningful information about their training sets. Such information leakage can be exploited, for example, by a vendor to build more effective classifiers or to simply acquire trade secrets from a competitor's apparatus, potentially violating its intellectual property rights.},
	pages = {137--150},
	number = {3},
	journaltitle = {International Journal of Security and Networks},
	author = {Ateniese, Giuseppe and Mancini, Luigi V. and Spognardi, Angelo and Villani, Antonio and Vitali, Domenico and Felici, Giovanni},
	urldate = {2022-11-12},
	date = {2015-01},
	note = {Publisher: Inderscience Publishers},
	keywords = {security, attack methodology, classifier hacking, hacking attacks, information leakages, intellectual property rights, {IPR}, machine learning classifiers, meta-classifiers, trade secrets, training sets, unauthorised access},
	file = {Ateniese et al_2015_Hacking smart machines with smarter ones.pdf:D\:\\Zetero\\storage\\L997I9IV\\Ateniese et al_2015_Hacking smart machines with smarter ones.pdf:application/pdf},
}

@misc{ateniese_hacking_2013,
