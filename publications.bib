	shortjournal = {Computers \& Industrial Engineering},
	author = {Verma, A. and Asadi, A. and Yang, K. and Maitra, A. and Asgeirsson, H.},
	urldate = {2023-11-09},
	date = {2019-02-01},
	keywords = {Feature extraction, Classification, Advanced metering infrastructure ({AMI}), Data-mining, Plug-in electric vehicles ({PEVs}), Residential customer loading, Time of usage ({TOU})},
	file = {ScienceDirect Full Text PDF:D\:\\Zetero\\storage\\E6JRR547\\Verma et al. - 2019 - Analyzing household charging patterns of Plug-in e.pdf:application/pdf;ScienceDirect Snapshot:D\:\\Zetero\\storage\\KEYSUWNZ\\S0360835218303656.html:text/html},
}

@article{zhao_quantifying_2019,
