	keywords = {deep learning, Feature extraction, Training, Neural networks, Big data, Algorithm design and analysis, Approximation algorithms, big data modeling, broad learning system ({BLS}), Heuristic algorithms, incremental learning, Learning systems, random vector functional-link neural networks ({RVFLNN}), single layer feedforward neural networks ({SLFN}), singular value decomposition ({SVD})},
