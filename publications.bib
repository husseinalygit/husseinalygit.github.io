@article{li_regional_2020,
	title = {Regional non-intrusive electric vehicle monitoring based on graph signal processing},
	volume = {14},
	rights = {© 2020 The Authors. {IET} Generation, Transmission \& Distribution published by John Wiley \& Sons, Ltd. on behalf of The Institution of Engineering and Technology},
